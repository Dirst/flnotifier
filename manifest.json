{
  "manifest_version": 2,
  "name": "Уведомитель проектов FL.RU",
  "version": "1.0",
  "icons": {
    "128": "128x128.png"
  },
  "permissions": [
    "https://www.fl.ru/*",
    "notifications",
    "storage"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Посмотреть проекты",
    "default_icon": "128x128.png",
    "default_popup": "popup.html"
  }
}